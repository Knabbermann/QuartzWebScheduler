@page
@model QuartzWebScheduler.Web.Areas.Quartz.Pages.Quartz_Logs.DetailsModel
@{
}

<div class="container p-3">
    <form method="post">
        <div class="row pt-4 align-items-center">
            <div class="row pb-2">
                <div class="col">
                    <h2 class="text-primary d-inline-block">Quartz Log Details</h2>
                </div>
            </div>
            <hr />

            <div class="form-floating py-2 col-6">
                <input asp-for="QuartzLog.QuartzJobConfig.JobName" class="form-control" aria-required="true" disabled=""/>
                <label asp-for="QuartzLog.QuartzJobConfig.JobName"></label>
            </div>
            <div class="form-floating py-2 col-6">
                <input asp-for="QuartzLog.QuartzJobConfig.GroupName" class="form-control" aria-required="true" disabled="" />
                <label asp-for="QuartzLog.QuartzJobConfig.GroupName"></label>
            </div>
            <div class="form-floating py-2 col-6">
                <input asp-for="QuartzLog.CronExpression" class="form-control" aria-required="true" disabled="" />
                <label asp-for="QuartzLog.CronExpression"></label>
            </div>
            <div class="form-floating py-2 col-6">
                <input asp-for="QuartzLog.Date" class="form-control" aria-required="true" disabled="" />
                <label asp-for="QuartzLog.Date"></label>
            </div>
            <hr />
            <div class="form-floating py-2 col-3">
                <input asp-for="QuartzLog.Statuscode" class="form-control" aria-required="true" disabled="" />
                <label asp-for="QuartzLog.Statuscode"></label>
            </div>
            <div class="form-floating py-2 col-9">
                <input asp-for="QuartzLog.Message" class="form-control" aria-required="true" disabled="" />
                <label asp-for="QuartzLog.Message"></label>
            </div>
            <div class="form-floating py-2 col-3">
                <input asp-for="QuartzLog.RequestType" class="form-control" aria-required="true" disabled="" />
                <label asp-for="QuartzLog.RequestType"></label>
            </div>
            <div class="form-floating py-2 col-9">
                <input asp-for="QuartzLog.RequestUrl" class="form-control" aria-required="true" disabled="" />
                <label asp-for="QuartzLog.RequestUrl"></label>
            </div>
            <div class="form-floating py-2 col-12">
                <input asp-for="QuartzLog.RequestBody" class="form-control" aria-required="true" disabled="" />
                <label asp-for="QuartzLog.RequestBody"></label>
            </div>
            <div class="form-floating py-2 col-6">
                <a class="btn btn-outline-secondary w-100" asp-area="Quartz" asp-page="/Quartz_Logs/Index">Back to list</a>
            </div>
        </div>
    </form>
</div>

@section Scripts
{
    <partial name="_ValidationScriptsPartial" />
}
